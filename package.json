{
  "name": "calltalent_web_cyclejs",
  "version": "1.0.0",
  "description": "Web Client for CallTalent powered by Cycle.js",
  "main": "index.js",
  "scripts": {
    "preinstall": "mkdirp dist && typings install && tsc",
    "webpack": "webpack-dev-server",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "start": "npm install && npm run webpack && echo 'OPEN index.html IN YOUR BROWSER'"
  },
  "dependencies": {
    "@cycle/dom": "^11.0.1",
    "@cycle/http": "^10.0.0",
    "@cycle/isolate": "^1.4.0",
    "@cycle/xstream-run": "^3.0.3",
    "material-design-lite": "^1.1.3",
    "ramda": "^0.21.0",
    "typescript": "^1.8.10",
    "xstream": "^5.3.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-preset-es2015-webpack": "^6.4.2",
    "babel-preset-stage-2": "^6.11.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-config-kentcdodds": "^8.1.3",
    "eslint-loader": "^1.5.0",
    "mkdirp": "0.5.x",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "typings": "^1.3.2",
    "webpack": "^2.1.0-beta.20",
    "webpack-validator": "^2.2.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wpcfan/calltalent_web_cyclejs.git"
  },
  "keywords": [
    "cyclejs",
    "node",
    "calltalent"
  ],
